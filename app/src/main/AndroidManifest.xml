<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.asraii.testchart" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MPAndroidChartActivity"
            android:label="@string/title_activity_mpandroid_chart"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HelloChartActivity"
            android:label="@string/title_activity_hello_chart"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".WilliamsChartActivity"
            android:label="@string/title_activity_williams_chart"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AndroidPlotActivity"
            android:label="@string/title_activity_android_plot"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AChartActivity"
            android:label="@string/title_activity_achart"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
    </application>

</manifest>
